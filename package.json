{
  "name": "hapi-orchestra-view",
  "version": "3.0.8",
  "description": "Library for composing handlebar html templates into a single page",
  "main": "lib/index.js",
  "scripts": {
    "test": "node_modules/.bin/lab -r lcov lib/test/index.js | ./node_modules/.bin/coveralls",
    "test-local": "./node_modules/.bin/lab -l lib/test/index.js",
    "test-cov-html": "./node_modules/.bin/lab -m 10000 -l -r html -o lib/test/coverage.html lib/test/index.js ",
    "jsdoc-to-markdown": "jsdoc2md --template README.hbs --files ./lib/index.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mickelindahl/hapi_orchestra_view.git"
  },
  "keywords": [
    "handlebars",
    "mustache",
    "template",
    "view",
    "pages",
    "web-pages",
    "hapi"
  ],
  "author": "Mikael Lindahl <mikael.lindahl.07@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mickelindahl/hapi_orchestra_view/issues"
  },
  "homepage": "https://github.com/mickelindahl/hapi_orchestra_view#readme",
  "devDependencies": {
    "code": "^5.1.2",
    "coveralls": "^3.0.0",
    "fs-extra": "^7.0.1",
    "hapi": "^18.1.0",
    "lab": "^18.0.2",
    "vision": "^5.3.0",
    "jsdoc-to-markdown": "4.0.1"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "handlebars": "^4.0.6",
    "joi": "^14.3.1"
  }
}
